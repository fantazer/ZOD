		display block
		color braun
	.container
		+below(640px)
			flex-wrap wrap
			justify-content center
		flex(between)
		.button-line-item
			+below(1050px)
				height 55px
			+below(640px)
				height 55px
				width 48%
				margin-right 5px
				margin-bottom 10px
