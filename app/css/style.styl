a
	text-decoration: none!important

ul
	padding 0

.dotted
	border-bottom 1px dotted braun
	
.container
	center(1200px)
	padding 10px

.header 
	bgi('header-bg.png')
	padding 10px 0

.header-info 
	flex(between)


.header-info-left
	flex(mid)


.header-logoBlock-text 
	font-size 14px
	font-weight 700
	color: #5b3d0c;
	margin-top 10px
	tshadow(white)

.header-slogan 
	+below(1050px)
		display none
	font-size 18px
	color braun
	font(zopa)
	margin-left 80px 
	tshadow(white)


.header-contact-number 
	font-size 28px
	font(pt)
	font-weight bold

.header-contact-callback 
	display block
	color white
	width 100%
	background green
	text-align center
	padding 5px
	text-transform uppercase
	font-size 14px
	font(pt)
	border-radius 3px	
	font-weight bold
	margin 5px 0 

.header-contact-text 
	font-size 16px
	font(ubuntu)
	font-weight 400
	text-align center
	color braun
	tshadow(white)

